{
  "root": "build/",
  "routes": {
    "/**": "index.html"
  },
  "https_only": true,
  "headers": {
    "/**": {
      "Strict-Transport-Security": "max-age=31557600"
    },
    "/": {
      "Cache-Control": "no-store, no-cache"
    },
    "**.css": {
      "Cache-Control": "public, max-age=31536000"
    },
    "**.svg": {
      "Cache-Control": "public, max-age=31536000"
    }
  }
}